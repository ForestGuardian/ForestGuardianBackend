    / add
    #pl.add_place.none
      .place_form
        %i#close.fa.fa-times.close_window
        %h3
          Add new place
          %span
        %form
          %label
            Place name:
            %input{:type => "text"}/
          %label
            latitude:
            %input{:type => "text"}/
          %label
            longitude:
            %input{:type => "text"}/
          %label
            Categori:
            %select
              %option{:value => "Cafe"} Cafe
              %option{:value => "Bar"} Bar
              %option{:value => "Cinema"} Cinema
              %option{:value => "Shop"} Shop
              %option{:value => "Club"} Club
              %option{:value => "Bank"} Bank
          %a#add.green_btn_header{:href => "#"} Add
